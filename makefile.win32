# $Id: makefile.win32,v 2.4 1996/10/03 21:02:37 layer Exp $

!include <ntwin32.mak>

emacs_dir = $(emacs_dir)

.c{$(BLD)}.obj:
	$(CC) -c -nologo -DWIN32 $(CFLAGS) -Fo$@ $<

default:	compile_el clman.exe

compile_el:
	$(emacs_dir)\bin\emacs -batch -l fi-compile.el -kill

clman.exe:	clman.obj clmanaux.obj
	link -nologo -out:$@ -subsystem:console -entry:mainCRTStartup \
		clman.obj clmanaux.obj wsock32.lib
